    <title>Arizona U.S. Export Assistance Centers Contact Information</title>
    <short_title>Arizona USEAC Contact Information</short_title>
    <summary>Contact Arizona's U.S. Export Assistance Centers.</summary>
    <creation_date>9/14/2014</creation_date>
    <release_date/>
    <expiration_date>9/13/2015</expiration_date>
    <source_agencies>
      <source_agency>International Trade Administration<source_business_unit>Global Markets</source_business_unit></source_agency>
    </source_agencies>
